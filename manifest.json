{
	"manifest_version":2,
	"name":"Bookmarkie",
	"version":"0.0.1",
	"permissions": [ 
		"bookmarks"
	],
	"description":"Lists Bookmarks",
	"icons":{
		"16": "images/bookmarkie32.png",
        "48": "images/bookmarkie48.png",
    	"128": "images/bookmarkie128.png"
	},
	
	"browser_action":{
		"default_icon":"images/bookmarkie32.png",
		"default_title":"Bookmarkie",
		"default_popup":"popup.html"
	}

}
